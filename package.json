{
	"name": "ue4-material-expression-generator",
	"publisher": "microdee",
	"displayName": "UE4 Material Expression Generator",
	"description": "Generate Material Expression nodes from HLSL code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.generateUe4Node"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateUe4Node",
				"title": "Generate UE4 Material Expression"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"postinstall": "typings install --save"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"@types/mustache": "latest",
		"@types/clipboardy": "2.0.1",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0",
		"typings": "latest"
	},
	"dependencies": {
		"mustache": "3.0.1",
		"clipboardy": "2.1.0"
	}
}
